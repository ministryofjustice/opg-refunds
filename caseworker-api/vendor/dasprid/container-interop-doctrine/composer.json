{
    "name": "dasprid/container-interop-doctrine",
    "type": "library",
    "description": "Doctrine factories for container-interop",
    "homepage": "https://github.com/DASPRiD/container-interop-doctrine",
    "license" : "BSD-2-Clause",
    "require": {
        "php": "^5.5|^7.0",
        "doctrine/orm": "^2.5",
        "psr/container": "^1.0.0",
        "doctrine/dbal": "^2.5",
        "doctrine/common": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "ContainerInteropDoctrine\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload-dev": {
        "psr-4": {
            "ContainerInteropDoctrineTest\\": "test/"
        }
    }
}
